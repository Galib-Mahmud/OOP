
package ooplab;


public class Overloading {
    void sum(double a,double b)
    {
        double sum=a+b;
        System.out.println(sum);
    }
    void sum(int a,int b,int c)
    {
        int sum=a+b+c;
        System.out.println(sum);
    }
    public static void main(String[] args) {
        Overloading ob=new Overloading();
        ob.sum(21, 22);
        ob.sum(21, 5, 4);
    }
}
