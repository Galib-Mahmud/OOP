
package opplab;

import java.util.Scanner;

public class PrimeNumber {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);
        int num;
           int count = 0;
        num=input.nextInt();
        for(int i=2; i<num;i++){
        if(num%i==0){
         
            count++;
            break;
        }
        
    } 
        if(count==0){
            System.out.println("Prime number");
                    }
        else{
            System.out.println("Not prime");
                 
        }
        
 
    }
         
    
}











M theke n number projonto prime number





package opplab;

import java.util.Scanner;


public class MtoNprime {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);
        int m,n,count=0;
        System.out.println("Enter starting number");
        m=input.nextInt();
        System.out.println("Enter ending number");
        n=input.nextInt();
        for(int i=m;i<=n;i++)// s theke e projonto sonkha gula input nibe
        {
            for(int j=2;j<=i-1;j++)//2 theke suru kore oi sonkha projonto 
            
            {
                if(i%j==0)
                {
                    count++;
                break;
                }
            }
            if(count==0)
                System.out.println(i );
            count=0;
        }
    }
      
    
}


